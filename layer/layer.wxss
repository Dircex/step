.luck_draw, .myCode, .followTemp {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  z-index: 100;
}

.closeDraw {
  width: 583rpx;
  position: absolute;
  top: 620rpx;
  left: 50%;
  margin-left: -291rpx;
}

.close_line {
  width: 1px;
  height: 100rpx;
  border-left: 1px white solid;
  margin-left: 80%;
}

.close {
  position: absolute;
  width: 60rpx;
  height: 166rpx;
  left: 80%;
  margin-left: -30rpx;
  top: 0%;
}

.drawBody {
  width: 583rpx;
  height: 790rpx;
  -webkit-animation: suofang linear 0.2s forwards;
  animation: suofang linear 0.2s forwards;
  position: absolute;
  top: 150rpx;
  left: 50%;
  margin-left: -291rpx;
  overflow: hidden;
}

.drawMain {
  width: 583rpx;
  height: 620rpx;
  background-color: white;
  border-radius: 12rpx;
  position: absolute;
  top: 0;
}

@-webkit-keyframes suofang {
  0% {
    -webkit-transform: scale(0);
    transform: scale(0);
  }

  50% {
    -webkit-transform: scale(1.1);
    transform: scale(1.1);
  }

  100% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

@keyframes suofang {
  0% {
    -webkit-transform: scale(0);
    transform: scale(0);
  }

  50% {
    -webkit-transform: scale(1.1);
    transform: scale(1.1);
  }

  100% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}


@-webkit-keyframes buttomtotop {
  0% {
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
  }

  50% {
    -webkit-transform: translateY(-45rpx);
    transform: translateY(-45rpx);
  }

  100% {
    -webkit-transform: translateY(-100rpx);
    transform: translateY(-100rpx);
  }
}
@keyframes buttomtotop {
  0% {
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
  }

  50% {
    -webkit-transform: translateY(-45rpx);
    transform: translateY(-45rpx);
  }

  100% {
    -webkit-transform: translateY(-100rpx);
    transform: translateY(-100rpx);
  }
}



